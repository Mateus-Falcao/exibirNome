let nomes = ["Ana", "Carlos", "Pedro", "Maria", "João"];
console.log("Terceiro nome:", nomes[2]);

nomes.push("Lucas");
nomes.unshift("Roberta");
console.log("Array atualizado com novos nomes:", nomes);

nomes.pop();
console.log("Array após remover o último nome:", nomes);
let numeros = [2, 4, 6, 8];
let numerosDobrados = numeros.map(num => num * 2);
console.log("Novo array com valores dobrados:", numerosDobrados);

let numerosFiltro = [1, 3, 5, 7, 9];
let numerosMaioresQueCinco = numerosFiltro.filter(num => num > 5);
console.log("Novo array com números maiores que 5:", numerosMaioresQueCinco);
